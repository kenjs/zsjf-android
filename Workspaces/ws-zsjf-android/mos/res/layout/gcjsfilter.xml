<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFFFF" >

    <com.cattsoft.mos.view.SelectLabelText
        android:id="@+id/gcjs_start_time"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="10dp"
        bgImage="@drawable/select_input_bg2"
        label="@string/gcjs_start_time"
        android:paddingLeft="10.0dip" />

    <com.cattsoft.mos.view.SelectLabelText
        android:id="@+id/gcjs_end_time"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gcjs_start_time"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="10dp"
        bgImage="@drawable/select_input_bg2"
        label="@string/gcjs_end_time"
        android:paddingLeft="10.0dip" />

    <com.cattsoft.mos.view.EditLabelText
        android:id="@+id/gcjs_requ_dept"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gcjs_end_time"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="10dp"
        label="@string/gcjs_requ_dept"
        android:paddingLeft="10.0dip" />
     <Button
        android:id="@+id/btn_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/gcjs_requ_dept"
        android:layout_marginTop="10.0dip"
        android:background="@drawable/btn_sure"
        android:textColor="#FFFFFFFF"
        android:textSize="16dip" />

</RelativeLayout>